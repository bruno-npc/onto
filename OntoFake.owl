############################
#   Object Properties
############################

# Object Property: :Autor (:Autor)

SubObjectPropertyOf(:Autor :Divulgador)
ObjectPropertyDomain(:Autor :Informação)
ObjectPropertyRange(:Autor :Autor)

# Object Property: :Data (:Data)

SubObjectPropertyOf(:Data :Divulgador)
ObjectPropertyDomain(:Data :Informação)
ObjectPropertyRange(:Data :Data)

# Object Property: :Divulgador (:Divulgador)

ObjectPropertyDomain(:Divulgador :Informação)
ObjectPropertyRange(:Divulgador :Noticias)

# Object Property: :Fake (:Fake)

InverseObjectProperties(:Fake :Fato)
ObjectPropertyDomain(:Fake :Fake)
ObjectPropertyRange(:Fake :Boatos)
ObjectPropertyRange(:Fake :Caluniar)
ObjectPropertyRange(:Fake :Capcioso)
ObjectPropertyRange(:Fake :LesãoÀFéPública)
ObjectPropertyRange(:Fake :MidiáticaEnganosa)
ObjectPropertyRange(:Fake :Parórias)
ObjectPropertyRange(:Fake :TeoriasNãoCientíficas)

# Object Property: :Fato (:Fato)

ObjectPropertyDomain(:Fato :Fato)
ObjectPropertyRange(:Fato :AutorCredibilizado)
ObjectPropertyRange(:Fato :DataPublicaçãoReal)
ObjectPropertyRange(:Fato :PortalComunicadorCredibilizado)
ObjectPropertyRange(:Fato :TítuloReal)

# Object Property: :Portal (:Portal)

SubObjectPropertyOf(:Portal :Divulgador)
ObjectPropertyDomain(:Portal :Informação)
ObjectPropertyRange(:Portal :Portal)

# Object Property: :Título (:Título)

SubObjectPropertyOf(:Título :Divulgador)
ObjectPropertyDomain(:Título :Informação)
ObjectPropertyRange(:Título :Título)



############################
#   Classes
############################

# Class: :Autor (:Autor)

SubClassOf(:Autor :Informação)

# Class: :AutorCredibilizado (:AutorCredibilizado)

SubClassOf(:AutorCredibilizado :Fato)

# Class: :Boatos (:Boatos)

SubClassOf(:Boatos :Descontextualizar)

# Class: :Caluniar (:Caluniar)

SubClassOf(:Caluniar :Descontextualizar)

# Class: :Capcioso (:Capcioso)

SubClassOf(:Capcioso :Demagogia)

# Class: :ClickBait (:ClickBait)

SubClassOf(:ClickBait :Falsidade)

# Class: :Data (:Data)

SubClassOf(:Data :Informação)

# Class: :DataPublicaçãoReal (:DataPublicaçãoReal)

SubClassOf(:DataPublicaçãoReal :Fato)

# Class: :Demagogia (:Demagogia)

SubClassOf(:Demagogia :Fake)

# Class: :Descontextualizar (:Descontextualizar)

SubClassOf(:Descontextualizar :Fake)

# Class: :FENAJ (:FENAJ)

AnnotationAssertion(rdfs:comment :FENAJ "Federação Nacional dos Jornalistas")
SubClassOf(:FENAJ :AutorCredibilizado)

# Class: :FIJ (:FIJ)

AnnotationAssertion(rdfs:comment :FIJ "Federação Internacional dos Jornalistas")
SubClassOf(:FIJ :AutorCredibilizado)

# Class: :Fake (:Fake)

SubClassOf(:Fake :Noticias)
DisjointClasses(:Fake :Fato)

# Class: :Falsidade (:Falsidade)

SubClassOf(:Falsidade :Fake)

# Class: :Fato (:Fato)

SubClassOf(:Fato :Noticias)

# Class: :LesãoÀFéPública (:LesãoÀFéPública)

SubClassOf(:LesãoÀFéPública :Demagogia)

# Class: :MidiáticaEnganosa (:MidiáticaEnganosa)

SubClassOf(:MidiáticaEnganosa :Falsidade)

# Class: :Parórias (:Parórias)

SubClassOf(:Parórias :Descontextualizar)

# Class: :Portal (:Portal)

SubClassOf(:Portal :Informação)

# Class: :PortalComunicadorCredibilizado (:PortalComunicadorCredibilizado)

SubClassOf(:PortalComunicadorCredibilizado :Fato)

# Class: :TeoriasNãoCientíficas (:TeoriasNãoCientíficas)

SubClassOf(:TeoriasNãoCientíficas :Falsidade)

# Class: :Título (:Título)

SubClassOf(:Título :Informação)

# Class: :TítuloReal (:TítuloReal)

SubClassOf(:TítuloReal :Fato)


