#   Named Individuals
############################

# Individual: :G1 (:G1)

ClassAssertion(:PortalComunicadorCredibilizado :G1)
ObjectPropertyAssertion(:Portal :G1 :Informação)

# Individual: :Informação (:Informação)

ClassAssertion(:Fato :Informação)

# Individual: :InformaçãoFalsa (:InformaçãoFalsa)

ClassAssertion(:Fake :InformaçãoFalsa)

# Individual: :PortalDuvidoso (:PortalDuvidoso)

ClassAssertion(:MidiáticaEnganosa :PortalDuvidoso)
ObjectPropertyAssertion(:Portal :PortalDuvidoso :InformaçãoFalsa)

# Individual: :TesteValidaçõesFalsas (:TesteValidaçõesFalsas)

ClassAssertion(:MidiáticaEnganosa :TesteValidaçõesFalsas)
ClassAssertion(:Parórias :TesteValidaçõesFalsas)
ObjectPropertyAssertion(:Portal :TesteValidaçõesFalsas :InformaçãoFalsa)


